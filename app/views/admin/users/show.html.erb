<div class="panel panel-default">
  <div class="panel-heading"><%= @user.name %></div>
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @user.name %></dd>
      <dt>Email</dt>
      <dd><%= @user.email %></dd>
      <dt>Chapter</dt>
      <dd><% if @user.chapter %><%= @user.chapter.name %><% end %></dd>
    </dl>
  </div>
</div>
